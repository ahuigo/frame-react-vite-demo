        listType="text"
        beforeUpload={beforeUpload}
        onRemove={onRemove}
        multiple={true}
        onChange={(info) => {
            setSelectedFile(info.fileList);
        }}
        showUploadList={true}
        fileList={selectedFile as unknown as UploadFile<unknown>[] | undefined}
        itemRender={(originNode, file) => (
            return <div>{file?.progress}</file>
        )}
    >



